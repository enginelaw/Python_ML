    "def sign(x):\n",
    "    if x<0:\n",
    "        sign='negative'\n",
    "    elif x>0:\n",
    "        sign='positive'\n",
    "    else:\n",
    "        sign='zero'\n",
    "    return sign\n",
    "\n",
    "A = 8.9\n",
    "print(f'Sign of {A} is {sign(A)}')"
